<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/infopage1"
    tools:context=".InfoPage1">

    <ImageButton
        android:id="@+id/backtomenubutton"
        android:layout_width="74dp"
        android:layout_height="61dp"
        android:layout_marginTop="608dp"
        android:layout_marginBottom="63dp"
        android:background="?android:attr/selectableItemBackground"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/homebutton" />

    <ImageButton
        android:id="@+id/rightbutton4"
        android:layout_width="74dp"
        android:layout_height="61dp"
        android:layout_marginTop="607dp"
        android:layout_marginBottom="63dp"
        android:background="?android:attr/selectableItemBackground"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/backtomenubutton"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rightbutton" />



</androidx.constraintlayout.widget.ConstraintLayout>